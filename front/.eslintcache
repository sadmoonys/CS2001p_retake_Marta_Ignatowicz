<Editing onClick={() => this.setState({ isInEdit: false, title: this.titlee.current.value, text: this.text.current.value })} />\r\n        <Cancel onClick={() => this.setState({ isInEdit: !this.state.isInEdit })} />\r\n      </section>\r\n      :\r\n      <section className=\"insideNote\">\r\n        <h4>{this.props.category}</h4>\r\n        <header>\r\n          <h3 className=\"titleNote\">{this.state.title}</h3>\r\n        </header>\r\n        <p className=\"textNote\">{this.state.text}</p>\r\n        <Delete onClick={this.delete.bind(this)} />\r\n        <Edit onClick={this.edit.bind(this)} />\r\n      </section>\r\n\r\n    );\r\n  }\r\n}\r\n\r\nexport default NoteCard;\r\n",["94","95"],"D:\\Uni work\\cs2001-2020_21-group15\\front\\src\\Components\\PageError\\PageError.jsx",[],{"ruleId":"96","replacedBy":"97"},{"ruleId":"98","replacedBy":"99"},{"ruleId":"100","severity":1,"message":"101","line":10,"column":8,"nodeType":"102","messageId":"103","endLine":10,"endColumn":22},{"ruleId":"104","severity":1,"message":"105","line":23,"column":30,"nodeType":"106","endLine":23,"endColumn":35},{"ruleId":"107","severity":1,"message":"108","line":33,"column":37,"nodeType":"109","messageId":"110","endLine":33,"endColumn":39},{"ruleId":"111","severity":1,"message":"112","line":57,"column":32,"nodeType":"113","endLine":57,"endColumn":48},{"ruleId":"107","severity":1,"message":"108","line":35,"column":23,"nodeType":"109","messageId":"110","endLine":35,"endColumn":25},{"ruleId":"107","severity":1,"message":"108","line":17,"column":26,"nodeType":"109","messageId":"110","endLine":17,"endColumn":28},{"ruleId":"100","severity":1,"message":"114","line":7,"column":8,"nodeType":"102","messageId":"103","endLine":7,"endColumn":18},{"ruleId":"96","replacedBy":"115"},{"ruleId":"98","replacedBy":"116"},"no-native-reassign",["117"],"no-negated-in-lhs",["118"],"no-unused-vars","'ProtectedRoute' is defined but never used.","Identifier","unusedVar","react/jsx-no-duplicate-props","No duplicate prop