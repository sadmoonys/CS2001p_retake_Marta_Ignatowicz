\n    );\n  }\n}\n\nexport default NoteCard;\n",{"ruleId":"79","severity":1,"message":"80","line":20,"column":30,"nodeType":"81","endLine":20,"endColumn":35},{"ruleId":"82","severity":1,"message":"83","line":42,"column":33,"nodeType":"84","endLine":42,"endColumn":64},{"ruleId":"82","severity":1,"message":"83","line":44,"column":28,"nodeType":"84","endLine":44,"endColumn":59},{"ruleId":"82","severity":1,"message":"83","line":45,"column":28,"nodeType":"84","endLine":45,"endColumn":59},{"ruleId":"85","severity":1,"message":"86","line":33,"column":37,"nodeType":"87","messageId":"88","endLine":33,"endColumn":39},{"ruleId":"89","severity":1,"message":"90","line":57,"column":32,"nodeType":"84","endLine":57,"endColumn":48},{"ruleId":"85","severity":1,"message":"86","line":18,"column":26,"nodeType":"87","messageId":"88","endLine":18,"endColumn":28},{"ruleId":"82","severity":1,"message":"83","line":15,"column":33,"nodeType":"84","endLine":15,"endColumn":64},{"ruleId":"82","severity":1,"message":"83","line":17,"column":28,"nodeType":"84","endLine":17,"endColumn":59},{"ruleId":"82","severity":1,"message":"83","line":18,"column":28,"nodeType":"84","endLine":18,"endColumn":59},{"ruleId":"91","severity":1,"message":"92","line":7,"column":8,"nodeType":"93","messageId":"94","endLine":7,"endColumn":18},"react/jsx-no-duplicate-props","No duplicate props allowed","JSXAttribute","jsx-a11y/anchor-is-valid","The href attribute requires a valid value to be accessible. Provide a valid, navigable address as the href value. If you cannot provide a valid href, but still need the element to resemble a link, use a button and change it with appropriate styles. Learn more: https://github.com/evcohen/eslint-plugin-jsx-a11y/blob/master/docs/rules/anchor-is-valid.md","JSXOpeningElement","eqeqeq","Expected '===' and instead saw '=='.","BinaryExpression","unexpected","jsx-a11y/alt-text","img elements must have an alt prop, either with meaningful text, or an empty string for decorative images.","no-unus